{% extends 'layout.html.twig' %}
{% set active = 'teammates' %}

{% block content %}

    Teammates list

    {% for teammate in teammates %}
        <br />
        Displaying teammate id : {{ teammate.id }}

        <br />Firstname : {{ teammate.firstname }}
        <br />Lastname : {{ teammate.lastname }}
        <br />Email : {{ teammate.email }}
        <br />Phone : {{ teammate.phone }}
        <br />Image : {{ teammate.image }}
    {% endfor %}


{% endblock %}